{
    "editor.fontFamily": "SF Mono Medium",
    "terminal.integrated.fontFamily": "'MesloLGMDZ Nerd Font Mono Regular'",
    "editor.fontLigatures": true,
    "workbench.colorTheme": "Evondev Dracula Normal Contrast",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "code-runner.runInTerminal": true,
  "editor.lineHeight": 30,
  "editor.fontSize": 16,
  "tabnine.experimentalAutoImports": false,
  "editor.suggest.preview": true,
  "editor.cursorSmoothCaretAnimation": "explicit",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "git.openRepositoryInParentFolders": "never",
  "update.mode": "start",
  "codesnap.backgroundColor": "#F999CB",
  "vscode_custom_css.imports": [
    "file:///C:/Users/Administrator/Documents/custom.css"
  ],
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["storage.type", "support.type"],
        "settings": {
          "fontStyle": "bold italic"
        }
      }
    ]
  },
  "editor.cursorBlinking": "smooth",
  "editor.cursorWidth": 2,
  "editor.cursorSurroundingLines": 1,
  "editor.cursorStyle": "line-thin",
  "editor.cursorSurroundingLinesStyle": "all",
  "terminal.integrated.cursorWidth": 12,
  "workbench.iconTheme": "material-icon-theme",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.letterSpacing": 0.5
}
